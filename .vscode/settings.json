{
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
  "editor.fontFamily": "'Jetbrains Mono', 'ProductSans', Monaco, 'Courier New', monospace, jetb",
  "prettier.bracketSameLine": true,
  "window.zoomLevel": 1,
  "editor.fontSize": 17,
  "editor.tabSize": 2,
  "editor.stickyScroll.enabled": true,
  "editor.inlayHints.enabled": "offUnlessPressed",
  "editor.wordWrap": "wordWrapColumn",
  "workbench.editor.wrapTabs": true,
  "html-css-class-completion.enableEmmetSupport": true,
  "emmet.triggerExpansionOnTab": true,
  "emmet.useInlineCompletions": true,
  "emmet.includeLanguages": {
    "sh": "bash",
    "html": "true",
    "json": "true",
    "yaml": "true",
    "svg": "true",
    "css": "true",
    "scss": "true"
  },
  "scss.format.spaceAroundSelectorSeparator": true,
  "html.format.indentInnerHtml": true,
  "javascript.format.insertSpaceAfterConstructor": true,
  "json.format.keepLines": false,
  "prettier.documentSelectors": [
    "**/*.svg",
    "**/*.mdx",
    "**/*.md",
    "**/*.svg",
    "**/*.json",
    "**/*.yml"
  ],
  "editor.formatOnSave": true,
  "editor.formatOnSaveMode": "file",
  "editor.formatOnPaste": true,
  "[markdown]": {
    "editor.quickSuggestions": {
      "comments": "on",
      "strings": "on",
      "other": "on"
    }
  },
  "files.associations": {
    "*.mdx": "markdown",
    "*.md": "markdown"
  },
  "colorize.include": [
    "**/*.html",
    "**/*.css",
    "**/*.scss",
    "**/*.sass",
    "**/*.less"
  ],
  "typescript.inlayHints.parameterNames.enabled": "all",
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "colorize.languages": [
    "md",
    "css",
    "sass",
    "scss",
    "less",
    "postcss",
    "sss",
    "stylus",
    "xml",
    "svg"
  ],
  "html.format.preserveNewLines": true,
  "html.format.maxPreserveNewLines": null,
  "markdown.preview.breaks": true,
  "editor.inlineSuggest.showToolbar": "always",
  "editor.suggest.localityBonus": true,
  "editor.suggest.preview": true,
  "editor.suggest.shareSuggestSelections": true,
  "scssFormatter.printWidth": 100,

  "editor.showFoldingControls": "always",
  "editor.folding": true,
  "editor.foldingMaximumRegions": 500,
  "editor.foldingHighlight": true,
  "editor.foldingStrategy": "auto",
  "html.format.preserveNewLines": true,
  "editor.wordBreak": "keepAll",
  "css.lint.float": "warning",
  "scss.lint.float": "warning",
  "debug.javascript.breakOnConditionalError": true,
  "markdown.preview.breaks": true
}
